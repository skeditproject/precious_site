<section class="portfolio-detail-modal" id="healthcaremodel">
  <div class="applysection-overlay"></div>
  <div class="portfolio-modal-form">
    <div class="d-flex flex-column position-relative">
      <p class="cancel-icon" onclick="cancelhealthCareModelOpen()">X</p>
      <h3 class="fw-bold text-center mt-3 mb-2">
        Health Care Management
      </h3>
      <h6 class="fw-bold text-center mb-5">
      </h6>
      <div style="max-height: 450px; overflow: auto; padding-inline: 12px;">
        <div class="d-flex gap-2 justify-content-between">
          <%= image_tag "theme1/healthcaremodel1.jpg", style: "border-radius: 12px; box-shadow: 0px 0px 5px lightgray; height: 220px; width: 100%; max-width: 32%; object-fit: cover;", alt: "" %>
          <%= image_tag "theme1/healthcaremodel2.jpg", style: "border-radius: 12px; box-shadow: 0px 0px 5px lightgray; height: 220px; width: 100%; max-width: 32%; object-fit: cover;", alt: "" %>
